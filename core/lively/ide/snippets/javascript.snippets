# Anonymous Function
snippet fn
regex /((=)\s*|(:)\s*|(\()|\b)/fn/
	function($2) {${0:$TM_SELECTED_TEXT}}${M2?;}${M3?,}
# Anonymous Function
regex /((=)\s*|(:)\s*|(\()|\b)/f2/(\))?/
name f2
