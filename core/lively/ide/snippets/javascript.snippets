# if
snippet if
	if (${1:true}) {
		${0}
	}
# if ... else
snippet ife
	if (${1:true}) {
		${2}
	} else {
		${0}
	}
snippet err
	if (${1:err}) return ${2:thenDo}(err, null);
snippet options
	if (typeof ${1:options} === "function") {
		${2:thenDo} = ${1};
		${1} = null;
	}
	${1} = ${1} || {};
# tertiary conditional
snippet ter
	${1:/* condition */} ? ${2:a} : ${3:b}
# switch
snippet switch
	switch (${1:expression}) {
		case '${3:case}':
			${4:// code}
			break;
		${5}
		default:
			${2:// code}
	}
# case
snippet case
	case '${1:case}':
		${2:// code}
		break;
	${3}

# while (...) {...}
snippet wh
	while (${1:/* condition */}) {${0:/* code */}}
# try
snippet try
